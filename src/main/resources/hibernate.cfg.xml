<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration PUBLIC   "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.dialect">org.hibernate.dialect.MySQL8Dialect</property>
		<property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
		<property name="hibernate.connection.url">jdbc:mysql://localhost:3306/criancaemfoco?createDatabaseIfNotExist=true&amp;serverTimezone=America/Sao_Paulo</property>
		<property name="hbm2ddl.auto">update</property>
		<property name="hibernate.connection.username">root</property>
		<property name="hibernate.connection.password">root</property>
		<property name="show_sql">true</property>
		<property name="format_sql">true</property>
		<property name="hibernate.current_session_context_class">org.hibernate.context.internal.ThreadLocalSessionContext</property>
		<mapping class="br.senac.criancaemfoco.modelo.entidade.agendamento.Agendamento" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.pessoa.responsavel.Responsavel" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.pessoa.aluno.Aluno" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.endereco.Endereco" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.pessoa.enfermeiro.Enfermeiro" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.escola.Escola" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.procedimento.Procedimento" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.turma.Turma" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.contato.Contato" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.estoque.Estoque" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.insumo.Insumo" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.papel.Papel" />
		<mapping class="br.senac.criancaemfoco.modelo.entidade.usuario.Usuario"/>
		<mapping class="br.senac.criancaemfoco.modelo.entidade.foto.Foto"/>
	</session-factory>
</hibernate-configuration>